<svg xmlns="http://www.w3.org/2000/svg" width="128" height="128" viewBox="0 0 128 128">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2DA6B2;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#21808D;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <rect width="128" height="128" rx="24" fill="url(#bg)"/>
  
  <g fill="white">
    <!-- Grid for Eisenhower Matrix -->
    <line x1="64" y1="20" x2="64" y2="108" stroke="white" stroke-width="3" opacity="0.8"/>
    <line x1="20" y1="64" x2="108" y2="64" stroke="white" stroke-width="3" opacity="0.8"/>
    
    <!-- Red quadrant (Do First) -->
    <circle cx="42" cy="42" r="8" fill="#FF5459"/>
    <text x="42" y="46" font-size="10" fill="white" text-anchor="middle" font-weight="bold">1</text>
    
    <!-- Yellow quadrant (Schedule) -->
    <circle cx="86" cy="42" r="8" fill="#FFB84D"/>
    <text x="86" y="46" font-size="10" fill="white" text-anchor="middle" font-weight="bold">2</text>
    
    <!-- Blue quadrant (Delegate) -->
    <circle cx="42" cy="86" r="8" fill="#4499FF"/>
    <text x="42" y="90" font-size="10" fill="white" text-anchor="middle" font-weight="bold">3</text>
    
    <!-- Green quadrant (Eliminate) -->
    <circle cx="86" cy="86" r="8" fill="#44BB44"/>
    <text x="86" y="90" font-size="10" fill="white" text-anchor="middle" font-weight="bold">4</text>
    
    <!-- Checkmark overlay -->
    <path d="M40 64 L56 80 L88 48" stroke="white" stroke-width="8" fill="none" stroke-linecap="round" stroke-linejoin="round" opacity="0.9"/>
  </g>
  
  <!-- Labels -->
  <text x="64" y="16" font-size="8" fill="white" text-anchor="middle" opacity="0.7">SÜRGŐS</text>
  <text x="12" y="68" font-size="8" fill="white" text-anchor="middle" opacity="0.7" transform="rotate(-90 12 68)">FONTOS</text>
</svg>
